<Window x:Class="TestRunner.GenerateDashboardWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Generating Dashboard" Height="80" Width="300" ResizeMode="NoResize" WindowStyle="ToolWindow" Loaded="Window_Loaded" WindowStartupLocation="CenterOwner">
    <Grid>
        <Grid.Background>
            <ImageBrush ImageSource="pack://siteoforigin:,,,/Resources/DeltekBackground.png" Stretch="UniformToFill"/>
        </Grid.Background>
        <StackPanel Orientation="Vertical" Margin="5,10,5,5">
            <ProgressBar x:Name="prgGenerateDashboard" Height="25"></ProgressBar>
        </StackPanel>
    </Grid>
</Window>
