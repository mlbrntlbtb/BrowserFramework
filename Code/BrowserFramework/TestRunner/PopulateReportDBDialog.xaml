<Window x:Class="TestRunner.PopulateReportDBDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Populate Report Database" Height="122" Width="375"
        ResizeMode="NoResize">
    <Grid>
        <Grid.Background>
            <ImageBrush ImageSource="pack://siteoforigin:,,,/Resources/DeltekBackground.png" Stretch="UniformToFill"/>
        </Grid.Background>
        <StackPanel Orientation="Vertical" Margin="2,5,2,5">
            <Label x:Name="lblStatus" Content="Importing Results [0 of 0]:" HorizontalAlignment="Left" />
            <Label x:Name="lblTestSuiteResult" Content="Test Suite Result" HorizontalAlignment="Center" />

        </StackPanel>
    </Grid>
</Window>
